pipeline {
    agent any

    stages {
        stage('Step One') {
            steps {
               bat 'dir c:\\windows > hasil.txt'
            }
        }
        stage('Step Two') {
            steps {
               bat 'copy hasil.txt d:\\latihan'
            }
        }
        stage('Step Three') {
            steps {
               bat 'del d:\\latihan\\result.txt'
               bat 'cd d:\\latihan'
               bat 'ren hasil.txt result.txt'
            }
        }
    }
}
